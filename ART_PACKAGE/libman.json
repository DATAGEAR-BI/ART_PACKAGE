    {
      "library": "@material/ripple@14.0.0",
      "destination": "wwwroot/lib/material/ripple/",
      "files": [
        "dist/mdc.ripple.css",
        "dist/mdc.ripple.css.map",
        "dist/mdc.ripple.js",
        "dist/mdc.ripple.js.map",
        "dist/mdc.ripple.min.css",
        "dist/mdc.ripple.min.css.map",
        "dist/mdc.ripple.min.js"
      ]
    },
    {
      "library": "bootstrap-icons@1.11.1",
      "destination": "wwwroot/lib/bootstrap-icons"
    }

  ]
}