@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@section KendoScripts{
    <partial name="_GridScripts" />
}



<section class="content-header">
    <h1>
        OS Liability Report
        <small class="h6">This report produces totals for outstanding liability</small>
        <br />
    </h1>
</section>

<m-grid id="OSLiability" data-urlkey="OSLiability" data-handlerkey="OSLiability" data-defaultGroup="@ViewBag.defaultGroup"></m-grid>

@section Scripts{
    <script type="module" defer src="~/js/components/grid/grid.js"></script>
}
