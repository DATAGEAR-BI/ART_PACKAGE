<m-grid id="GoamlReportsValidation" data-urlkey="GoamlReportsValidation" data-handlerkey="GoamlReportsValidation"></m-grid>

@section Scripts{
    <script type="module" defer src="~/js/components/grid/grid.js"></script>
}
