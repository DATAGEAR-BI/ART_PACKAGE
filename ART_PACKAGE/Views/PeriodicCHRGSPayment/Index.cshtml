@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@section KendoScripts{
    <partial name="_GridScripts" />
}



<section class="content-header">
    <h1>
        Periodic CHRGs Payment Report
        <small class="h6">This report produces all transactions which have a Pay Charges event within a period that you can specify</small>
        <br />
    </h1>
</section>

<m-grid id="PeriodicCHRGSPayment" data-urlkey="PeriodicCHRGSPayment" data-handlerkey="PeriodicCHRGSPayment"></m-grid>

@section Scripts{
    <script type="module" defer src="~/js/components/grid/grid.js"></script>
}
