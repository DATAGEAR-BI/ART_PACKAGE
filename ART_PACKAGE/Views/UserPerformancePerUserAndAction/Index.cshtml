@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<section class="content-header">
    <h1>
        User Performance Per User and Action Report
        <small> </small>
        <br />
    </h1>
</section>

<div class="row d-flex align-items-center">
    <smart-query-builder class="col-10" id="querybuilder" data-applybtn="applyBtn" data-params="UserPerformancePerUserAndAction"></smart-query-builder>
    <div class="col-2 d-flex justify-content-center">
        <button id="applyBtn" class="btn btn-primary">Apply</button>
    </div>
</div>
<m-grid id="UserPerformancePerUserAndAction" data-stored="querybuilder" data-urlkey="UserPerformancePerUserAndAction"></m-grid>


@section KendoScripts{
    <link href="~/lib/Plugins/Kendo/kendo.default-v2.min.css" rel="stylesheet" />
    <script src="~/lib/Plugins/Kendo/jquery-1.12.4.min.js"></script>
    <script src="~/lib/Plugins/Kendo/jquery.min.js"></script>
    <script src="~/lib/Plugins/Kendo/jszip.js"></script>
    <script src="~/lib/Plugins/Kendo/jszip.min.js"></script>
    <script src="~/lib/Plugins/Kendo/kendo.all.min.js"></script>
    <script src="~/lib/jquery-loading-overlay/loadingoverlay.js"></script>
}
@section Scripts {

    <script src="~/lib/smartcomponents/source/modules/smart.multicomboinput.js"></script>
    <script src="~/lib/smartcomponents/source/modules/smart.querybuilder.js"></script>
    <script type="module" defer src="~/js/components/grid/grid.js"></script>
}
