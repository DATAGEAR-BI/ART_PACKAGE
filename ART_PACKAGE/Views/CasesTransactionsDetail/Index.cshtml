<section class="content-header">
    <h1>
        Cases Transactions Detail
        <small>Presents all credit transactions for specific staff for only created cases with the related information as below</small>
        <br />
    </h1>
</section>
<filters-control data-key="CasesTransactionsDetail"></filters-control>
<div id="grid"></div>

<div class="modal fade" id="caseModal" tabindex="-1" role="dialog" aria-labelledby="caseModalLablel" aria-hidden="true">
    <div class="modal-dialog custom-modal-width" role="document" style="width:60%">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="caseTitle">Case History</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>

            </div>

            <div class="modal-body" id="caseModalBody">
                <div id="CaseInfo" style="max-height:333px;max-width:1463px;overflow:scroll">
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


@section Scripts {


    <script src="~/js/constructfilters.js" defer type="module"></script>
    <script src="~/js/dynamic.js" id="script" defer type="module" data-stored="true" data-urlkey="CasesTransactionsDetail" data-handlerkey="CasesTransactionsDetail"></script>
}
